ALT_SUFFIX <- ".GOLD"
GEN_SUFFIX <- ".GEN"
long <- "(.+)\\(.+\\)"
SHORT_MODEL_NAME <- ifelse(str_detect(MODEL_NAME,long), str_extract(MODEL_NAME,long,group=1),MODEL_NAME)
SHORT_ALT_NAME <- ifelse(str_detect(ALT_NAME,long), str_extract(ALT_NAME,long,group=1),ALT_NAME)
